  "main": "lib/index.js",
  "scripts": {
		"test": "echo 'Error: no test specified' && exit 1",
		"build": "mkdirp build && browserify -r ./:biojs-vis-pca -o build/pca.js",
		"build-browser": "npm run build",
		"prepublish": "npm run build",
		"watch": "watchify -r ./:biojs-vis-pca -v -o build/pca.js",
		"w": "prunner \"npm run sniper\" \"npm run watch\"",
		"sniper": "sniper ."
  },
  "dependencies": {},
  "devDependencies": {
		"mkdirp": "^0.5.0",
		"browserify": "6.x",
		"sniper": "0.x",
		"watchify": "^1.0.6",
		"prunner": "1.x"
  }, 
  "sniper": {
    "js": [ "/build/pca.js"],
    "snippets": ["examples"],
    "first": "simple"
   }, 
  "keywords": ["pca","biojs"]
}
